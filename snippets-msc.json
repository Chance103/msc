{
	"if": {
		"prefix": ["@if", "if"],
		"body": [
			"@if $1",
			"\t$0",
			"@fi"
		],
		"description": "If statement"
	},
	"if-else": {
		"prefix": ["@if", "if"],
		"body": [
			"@if $1",
			"\t$0",
			"@else",
			"\t",
			"@fi"
		],
		"description": "If-else statement"
	},
	"define": {
		"prefix": ["@define", "define"],
		"body": "@define ${1:Int} ${2:myVar} = ${3:0}",
		"description": "Define variable"
	},
	"player_print": {
		"prefix": ["@player", "player", "@print", "print"],
		"body": "@player ${1:hello}",
		"description": "Say message to player"
	},
	"chatscript": {
		"prefix": ["@chatscript", "chatscript"],
		"body": "@chatscript ${1:10s} ${2:groupname} ${3:do_stuff()}",
		"description": "Chatscript" 
	},
	"prompt": {
		"prefix": ["@prompt", "prompt"],
		"body": "@prompt ${1:10s} ${2:myStringVar} ${3:Prompt expired}",
		"description": "Chatscript" 
	},
	"delay": {
		"prefix": ["@delay", "delay"],
		"body": "@delay ${1:1s}",
		"description": "Delay"
	},
	"cooldown": {
		"prefix": ["@cooldown", "cooldown"],
		"body": "@cooldown ${1:1s}",
		"description": "Cooldown"
	},
	"global_cooldown": {
		"prefix": ["@global_cooldown", "global_cooldown"],
		"body": "@global_cooldown ${1:1s}",
		"description": "Global cooldown"
	},
	"for": {
		"prefix": ["@for", "for"],
		"body": [
			"@for ${1:Int} ${2:i} in ${3:list::range(0, 10)}",
			"\t$0",
			"@done"
		],
		"description": "For statement"
	}
}